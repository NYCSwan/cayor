{
  "name": "Cayor_Capital",
  "version": "0.1.0",
  "private": true,
  "description": "Cayor Capital is a Sub-Saharan Africa Equity Fund, investing in mid markets across Africa. The Fund directed by experienced African investors, provides a diverse portfolio in growing industries including Financial Services, Agriculture, Consumer goods, and infrastructure. Cayor is committed to ESG and secure returns for investors.",
  "keywords": [
    "Sub-Saharan Africa",
    "Africa",
    "equity fund",
    "investing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nycswan/cayor.git"
  },
  "engines": {
    "node": ">=8",
    "yarn": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.1.1",
    "node-sass-chokidar": "^1.2.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-transition-group": "^2.3.1",
    "reactstrap": "^6.0.1",
    "source-map-explorer": "^1.6.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "prestart": "npm install -g serve",
    "startdev": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --scripts-prepend-node-path",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "sass": "^1.3.0"
  }
}
